{
  "name": "react-slide-toggle",
  "version": "0.1.13",
  "dependencies": {},
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.build.js --mode production",
    "minify": "npm run minify-umd",
    "minify-all": "npm run minify-umd & npm run minify-var",
    "minify-umd": "uglifyjs dist/ReactSlideToggle.umd.js --config-file uglify.config.json --output dist/ReactSlideToggle.umd.min.js",
    "minify-var": "uglifyjs dist/ReactSlideToggle.var.js --config-file uglify.config.json --output dist/ReactSlideToggle.var.min.js",
    "deploy": "npm run build && npm run minify"
  },
  "description": "React component version of jQuery.slideToggle",
  "main": "dist/ReactSlideToggle.umd.min.js",
  "umd": "dist/ReactSlideToggle.umd.js",
  "unpkg": "dist/ReactSlideToggle.umd.min.js",
  "author": "kunukn",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": "last 2 versions, ie 11",
  "babel": {
    "presets": [
      "env",
      "stage-2",
      "react"
    ]
  },
  "peerDependencies": {
    "react": "^15.x || ^16.x || ^17.x",
    "react-dom": "^15.x || ^16.x || ^17.x"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.4",
    "babel-core": "6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "bezier-easing": "^2.1.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "eases": "^1.0.8",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "hoek": "^6.1.2",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "4.11.0",
    "parallel-webpack": "^2.3.0",
    "postcss-loader": "^3.0.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "3.1.14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/react-slide-toggle.git"
  },
  "keywords": [
    "slide",
