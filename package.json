{
  "name": "react-slide-toggle",
  "version": "0.3.3",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "clean": "rimraf dist/*.*",
    "start": "webpack-dev-server --color --mode development --http --env.PORT=3333",
    "build-es5": "rollup -c rollup.config.js --environment ES5",
    "build-es6": "rollup -c rollup.config.js --environment ES6",
    "build": "yarn clean && yarn build-es5 && yarn build-es6",
    "build:watch": "rollup -c rollup.config.js --environment ES5 -w",
    "webpack-build": "webpack --mode production",
    "validate": "webpack-dev-server --color --mode development --env.VALIDATE=1  --env.PORT=4444",
    "rebuild-node-sass": "npm rebuild node-sass",
    "test": "jest",
    "source-map": "source-map-explorer dist/Collapse.js"
  },
  "description": "React component version of jQuery.slideToggle",
  "main": "dist/ReactSlideToggle.umd.js",
  "module": "dist/ReactSlideToggle.umd.es2015.js",
  "cjs": "dist/ReactSlideToggle.cjs.js",
  "author": "kunukn",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {},
  "peerDependencies": {
    "react": "^15.x || ^16.x || ^17.x",
    "react-dom": "^15.x || ^16.x || ^17.x"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/jestSetup.js",
    "snapshotSerializers": [],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(xcss|xscss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "css",
      "scss"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "@reach/component-component": "^0.8.6",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-backgrounds": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@storybook/storybook-deployer": "^2.8.3",
    "@svgr/rollup": "^5.2.0",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bezier-easing": "^2.1.0",
    "browser-sync": "^2.26.7",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.4.2",
    "eases": "^1.0.8",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "file-loader": "^5.1.0",
    "gzip-size": "^5.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "normalize-scss": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prop-types": "15.7.2",
    "raw-loader": "^4.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^2.4.1",
    "rollup-plugin-scss": "^2.1.0",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-url": "^3.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-md5-hash": "0.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/react-slide-toggle.git"
  },
  "keywords": [
    "slide",
    "toggle",
    "collapsible"
  ],
  "bugs": {
    "url": "https://github.com/kunukn/react-slide-toggle/issues"
  },
  "homepage": "https://github.com/kunukn/react-slide-toggle#readme"
}
